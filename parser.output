Grammar

    0 $accept: expression $end

    1 expression: conjunction
    2           | expression OR conjunction

    3 conjunction: conjunction AND equality
    4            | equality

    5 equality: equality "==" relational
    6         | equality "!=" relational
    7         | relational

    8 relational: relational ">" term
    9           | relational ">=" term
   10           | relational "<" term
   11           | relational "<=" term
   12           | term

   13 term: term '+' factor
   14     | term '-' factor
   15     | factor

   16 factor: factor '*' power
   17       | factor '/' power
   18       | factor '%' power
   19       | power

   20 power: power '^' unary
   21      | unary

   22 unary: NOT unary
   23      | '-' unary
   24      | abs

   25 abs: '|' abs '|'
   26    | literal

   27 literal: '(' expression ')'
   28        | INT
   29        | DBL
   30        | TRUE
   31        | FALSE
   32        | IDENT


